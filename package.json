{
  "name": "tweetnacl",
  "version": "1.0.0",
  "description": "Port of TweetNaCl cryptographic library to JavaScript es6",
  "main": "nacl-fast-es.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-all": "node -r esm test/test-all.js",
    "test-quick": "node -r esm test/test-quick.js",
    "test-browser": "node -r esm test/helpers/browser-quick-start /test/browser/index.html",
    "bench": "node -r esm test/benchmark/bench.js",
    "bench-browser": "node -r esm test/helpers/browser-quick-start /test/benchmark/bench.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hakanols/tweetnacl-js.git"
  },
  "keywords": [
    "crypto",
    "cryptography",
    "curve25519",
    "ed25519",
    "encrypt",
    "hash",
    "key",
    "nacl",
    "poly1305",
    "public",
    "salsa20",
    "signatures"
  ],
  "author": "TweetNaCl-js contributors",
  "license": "Unlicense",
  "homepage": "https://tweetnacl.js.org",
  "devDependencies": {
    "esm": "^3.2.25",
    "child_process": "^1.0.2"
  }
}
